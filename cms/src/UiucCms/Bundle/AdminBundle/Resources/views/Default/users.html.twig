{% extends "UiucCmsAdminBundle::base.html.twig" %}

{% block body %}
<div class="container">
    <div class="page-header" style="text-align:left">
        <h1>
            Users:
        </h1>
    </div>
    <p class="lead">
        {% for user in users %}
            <li>{{ user.getUserName() }} {{ user.getEmail }} </li><br>
        {% endfor %}
    </p>
</div>
{% endblock %}